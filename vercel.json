{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node",
        "config":{
          "nodeVersion":"18.16.0"
        }
      },
      {
        "src": "client/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "client/build"
        }
      }

    ],
  "routes":[
    {
      "src":"/api/(.*)",
      "dest":"/server.js"
    },
    {
      "src":"/(.*)",
      "dest":"/client/build/$1"
    }
  ]
  }
  